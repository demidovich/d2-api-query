{
    "name": "demidovich/d2-api-query",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "php",
        "api query",
        "laravel query builder"
    ],
    "autoload": {
        "psr-4": {
            "D2\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.2",
        "illuminate/database": "^6.0|^7.0|^8.0",
        "illuminate/validation": "^6.0|^7.0|^8.0",
        "illuminate/pagination": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=8.0",
        "squizlabs/php_codesniffer": ">=3.0",
        "symfony/var-dumper": ">=5.0",
        "illuminate/filesystem": ">=8.0",
        "illuminate/translation": ">=8.0",
        "vimeo/psalm": ">=4.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
